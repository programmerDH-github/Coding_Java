<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper

		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bside.BSIDE.user.persistence.UserMapper">
	<insert id="insertUser" parameterType="com.bside.BSIDE.user.domain.UserDto">
		INSERT INTO
			user.info(
			id,
			username,
			password,
			email
		)
		VALUES(
				  #{id},
				  #{username},
				  #{password},
				  #{email}
			  )
	</insert>
	
	<select id="getAllUsers" resultType="com.bside.BSIDE.user.domain.UserDto">
		SELECT * FROM user.info
	</select>
</mapper>